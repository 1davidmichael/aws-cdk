#!/bin/bash
# Wrapper for 'npm' which will append a strict version to module installation.
set -eu

install_command=$@

NPM_INSTALL_PACKAGE_SUFFIX=${NPM_INSTALL_PACKAGE_SUFFIX:$(CANDIDATE_VERSION)}

echo "We got here. Yey"
echo "${install_command}${NPM_INSTALL_PACKAGE_SUFFIX}"
exit 1

exec $ORIGINAL_NPM ${install_command}${NPM_INSTALL_PACKAGE_SUFFIX}
